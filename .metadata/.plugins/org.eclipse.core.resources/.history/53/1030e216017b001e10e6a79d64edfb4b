import java.util.Arrays;

public class Question3 {

	public static void main(String[] args) {
		int[] arr = {4,3, 3, 4, 21, 3, 21, 1, 7};
		Arrays.sort(arr);
		System.out.println(Arrays.toString(arr));
		boolean flag=false;
		
		for(int i=1; i<arr.length; i++) {
			if(arr[i]==arr[i-1]) {
				i++;
				flag = true;
				continue;
			}
			if(flag) {
				System.out.print(arr[i-1] + " ");
				flag=false;
			}
		}
	}

}
